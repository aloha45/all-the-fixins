<%- include('../partials/header') %>  
  <ul>Registered Users:
    <% users.forEach(u => { %>
      <a href="/users/<%= u._id %>">
        <div class="card">
          <div class="card-body">
            <img height="50" id="avatarPhoto" src="<%= u.avatar ? u.avatar : 'http://theoldreader.com/kittens/300/300/' %>" alt=""> 
            <%= u.name %></a> - Joined on <%= u.createdAt.toLocaleString().split(',')[0] %>
          </div>
        </div><br>
    <% }) %>
  </ul>
  <h1>Come on in guys!</h1>
  <h3>This is a user index page to see all users</h3>
<%- include('../partials/footer') %>